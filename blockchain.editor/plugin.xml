<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated blockchain -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="blockchain.presentation.BlockchainModelWizardID"
            name="%_UI_BlockchainModelWizard_label"
            class="blockchain.presentation.BlockchainModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/BlockchainModelFile.gif">
         <description>%_UI_BlockchainModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated blockchain -->
      <editor
            id="blockchain.presentation.BlockchainEditorID"
            name="%_UI_BlockchainEditor_label"
            icon="icons/full/obj16/BlockchainModelFile.gif"
            extensions="blockchain"
            class="blockchain.presentation.BlockchainEditor"
            contributorClass="blockchain.presentation.BlockchainActionBarContributor">
      </editor>
   </extension>

</plugin>
